<div id="shopify-section-sections">
  <link rel="stylesheet" href="{{ 'component-list-menu.css' | asset_url }}" media="print" onload="this.media='all'">
  <link rel="stylesheet" href="{{ 'component-search.css' | asset_url }}" media="print" onload="this.media='all'">
  <link rel="stylesheet" href="{{ 'component-menu-drawer.css' | asset_url }}" media="print" onload="this.media='all'">
  <link
    rel="stylesheet"
    href="{{ 'component-cart-notification.css' | asset_url }}"
    media="print"
    onload="this.media='all'"
  >

  {%- if settings.predictive_search_enabled -%}
    <link rel="stylesheet" href="{{ 'component-price.css' | asset_url }}" media="print" onload="this.media='all'">
    <link
      rel="stylesheet"
      href="{{ 'component-loading-overlay.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  <noscript>{{ 'component-list-menu.css' | asset_url | stylesheet_tag }}</noscript>
  <noscript>{{ 'component-search.css' | asset_url | stylesheet_tag }}</noscript>
  <noscript>{{ 'component-menu-drawer.css' | asset_url | stylesheet_tag }}</noscript>
  <noscript>{{ 'component-cart-notification.css' | asset_url | stylesheet_tag }}</noscript>
  {% render 'header-style' %}
  <script src="{{ 'details-disclosure.js' | asset_url }}" defer></script>
  <script src="{{ 'details-modal.js' | asset_url }}" defer></script>
  <script src="{{ 'cart-notification.js' | asset_url }}" defer></script>
  <script src="{{ 'search-form.js' | asset_url }}" defer="defer"></script>

  <svg xmlns="http://www.w3.org/2000/svg" class="hidden">
    <symbol id="icon-search" viewBox="0 0 48 48" fill="none" stroke-width="2">
      <title>search</title>
      <g transform="matrix(2,0,0,2,0,0)">
          <path fill="none" stroke-linecap="round" stroke-linejoin="round" d="M1.472 13.357a9.063 9.063 0 1 0 16.682-7.09 9.063 9.063 0 1 0-16.682 7.09"></path>
          <path fill="none" stroke-linecap="round" stroke-linejoin="round" d="m16.221 16.22 7.029 7.03"></path>
      </g>
    </symbol>
    <symbol id="icon-close" viewBox="0 0 48 48" fill="none" stroke-width="2">
        <title>close</title>
        <g transform="matrix(2,0,0,2,0,0)">
            <path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" d="m.75 23.249 22.5-22.5"></path>
            <path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" d="M23.25 23.249.75.749"></path>
        </g>
    </symbol>
  </svg>

  {% if section.settings.enable_sticky_header %}
    <sticky-header
      class="header-wrapper{% if section.settings.show_line_separator %} header-wrapper--border-bottom{% endif %}"
    >
  {% else %}
    <div class="header-wrapper{% if section.settings.show_line_separator %} header-wrapper--border-bottom{% endif %}">
  {% endif %}
  <header class="header header--{{ section.settings.logo_position }} page-width-desktop {% if section.settings.menu != blank %} header--has-menu{% endif %} {% if section.settings.hide_account_icon %}no-account-mobile{% endif %}">
    {%- if section.settings.menu != blank -%}
      <header-drawer data-breakpoint="tablet">
        <details class="menu-drawer-container">
          <summary
            class="header__icon header__icon--menu header__icon--summary link link--text focus-inset"
            aria-label="{{ 'sections.header.menu' | t }}"
          >
            <span>
              {% render 'icon-hamburger' %}
              {% render 'icon-close' %}
            </span>
          </summary>
          <div class="drawer_layer"></div>
          <div id="menu-drawer" class="menu-drawer motion-reduce" tabindex="-1">
            <div class="menu-drawer__inner-container">
              <div class="menu-drawer__navigation-container">
                <nav class="menu-drawer__navigation">
                  <ul class="menu-drawer__menu list-menu" role="list">
                    {%- for link in section.settings.menu.links -%}
                      <li>
                        {%- if link.links != blank -%}
                          <details>
                            <summary class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--active{% endif %}">
                              {{ link.title | escape }}
                              {% render 'icon-caret-nav' %}
                            </summary>
                            <div
                              id="link-{{ link.title | escape }}"
                              class="menu-drawer__submenu motion-reduce"
                              tabindex="-1"
                            >
                              <div class="menu-drawer__inner-submenu">
                                <button
                                  class="menu-drawer__close-button link link--text focus-inset"
                                  aria-expanded="true"
                                >
                                  {% render 'icon-caret-nav' %}
                                  <span class="title-escape"> {{ link.title | escape }} </span>
                                </button>
                                <ul class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                  {%- for childlink in link.links -%}
                                    <li>
                                      {%- if childlink.links == blank -%}
                                        <a
                                          href="{{ childlink.url }}"
                                          class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if childlink.current %} menu-drawer__menu-item--active{% endif %}"
                                          {% if childlink.current %}
                                            aria-current="page"
                                          {% endif %}
                                        >
                                          {{ childlink.title | escape }}
                                        </a>
                                      {%- else -%}
                                        <details>
                                          <summary class="menu-drawer__menu-item link link--text list-menu__item focus-inset">
                                            {{ childlink.title | escape }}
                                            {% render 'icon-caret-nav' %}
                                            {% render 'icon-caret' %}
                                          </summary>
                                          <div
                                            id="childlink-{{ childlink.title | escape }}"
                                            class="menu-drawer__submenu motion-reduce"
                                          >
                                            <button
                                              class="menu-drawer__close-button link link--text focus-inset mobile-menu-item"
                                              aria-expanded="true"
                                            >
                                              {% render 'icon-caret-nav' %}
                                              <span class="title-escape"> {{ childlink.title | escape }} </span>
                                              <a href="{{ childlink.url }}" class="view-all-anchor">
                                                <p class="view-all">{{ 'sections.header.view_all' | t }}</p>
                                              </a>
                                            </button>
                                            <ul class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                              {%- for grandchildlink in childlink.links -%}
                                                <li>
                                                  <a
                                                    href="{{ grandchildlink.url }}"
                                                    class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if grandchildlink.current %} menu-drawer__menu-item--active{% endif %}"
                                                    {% if grandchildlink.current %}
                                                      aria-current="page"
                                                    {% endif %}
                                                  >
                                                    {{ grandchildlink.title | escape }}
                                                  </a>
                                                </li>
                                              {%- endfor -%}
                                            </ul>
                                          </div>
                                        </details>
                                      {%- endif -%}
                                    </li>
                                  {%- endfor -%}
                                </ul>
                              </div>
                            </div>
                          </details>
                        {%- else -%}
                          <a
                            href="{{ link.url }}"
                            class="no-sub menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"
                            {% if link.current %}
                              aria-current="page"
                            {% endif %}
                          >
                            {{ link.title | escape }}
                          </a>
                        {%- endif -%}
                      </li>
                    {%- endfor -%}
                  </ul>
                </nav>
                <div class="menu-drawer__utility-links">
                  {%- if shop.customer_accounts_enabled -%}
                    <a
                      href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                      class="menu-drawer__account link link--text focus-inset h5"
                    >
                      {% render 'icon-account' %}
                      {%- liquid
                        if customer
                          echo 'customer.account_fallback' | t
                        else
                          echo 'customer.log_in' | t
                        endif
                      -%}
                    </a>
                    {%- if section.settings.enable_wishlist_icon -%}
                      <div class="mobile-wishlist">
                        <a
                          style="width:auto !important"
                          href="{%- if customer -%}{{ pages['wishlist'].url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                          class="mrg-cls menu-drawer-wishlist navigation__link header__icon link link--text focus-inset h5"
                        >
                          {%- render 'icon-heart' -%}
                          {{ link.title | escape }} Favourite
                        </a>
                      </div>
                    {%- endif -%}
                  {%- endif -%}
                  {% if settings.show_mobile_social_icons %}
                    <ul class="list list-social list-unstyled" role="list">
                      {%- if settings.social_twitter_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_twitter_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-twitter' -%}
                            <span class="visually-hidden">{{ 'general.social.links.twitter' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_facebook_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_facebook_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-facebook' -%}
                            <span class="visually-hidden">{{ 'general.social.links.facebook' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_pinterest_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_pinterest_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-pinterest' -%}
                            <span class="visually-hidden">{{ 'general.social.links.pinterest' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_instagram_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_instagram_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-instagram' -%}
                            <span class="visually-hidden">{{ 'general.social.links.instagram' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_tumblr_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_tumblr_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-tumblr' -%}
                            <span class="visually-hidden">{{ 'general.social.links.tumblr' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_snapchat_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_snapchat_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-snapchat' -%}
                            <span class="visually-hidden">{{ 'general.social.links.snapchat' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_youtube_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_youtube_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-youtube' -%}
                            <span class="visually-hidden">{{ 'general.social.links.youtube' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                      {%- if settings.social_vimeo_link != blank -%}
                        <li class="list-social__item">
                          <a
                            href="{{ settings.social_vimeo_link }}"
                            class="link social_link_nav list-social__link"
                            aria-describedby="a11y-external-message"
                          >
                            {%- render 'icon-vimeo' -%}
                            <span class="visually-hidden">{{ 'general.social.links.vimeo' | t }}</span>
                          </a>
                        </li>
                      {%- endif -%}
                    </ul>
                  {% endif %}
                </div>
                {% if section.settings.enable_currency_change %}
                  {% render 'footer-localization' %}
                {% endif %}
              </div>
            </div>
          </div>
        </details>
      </header-drawer>
    {%- endif -%}

    {%- if section.settings.logo_position == 'top-center'
      or section.settings.menu == blank
      or section.settings.logo_position == 'middle-center'
    -%}
      {%- render 'header-search', input_id: 'Search-In-Modal' -%}
    {%- endif -%}

    {%- if request.page_type == 'index' -%}
      <h1 class="header__heading">
    {%- else %}
      <h2 class="header__heading">
    {%- endif -%}
    <a href="{{ routes.root_url }}" class="header__heading-link link link--text focus-inset">
      {%- if section.settings.logo != blank -%}
        {%- assign logo_alt = section.settings.logo.alt | default: shop.name | escape -%}
        {%- assign logo_width = section.settings.logo_height | divided_by: section.settings.logo.aspect_ratio -%}
        {{
          section.settings.logo
          | image_url: width: section.settings.width
          | image_tag:
            class: 'header__heading-logo',
            height: '20, 50, 75, 125, 150, 200, 250, 300, 400',
            height: section.settings.logo_height,
            width: logo_width,
            alt: logo_alt
        }}
      {%- else -%}
        <span class="h2">{{ shop.name }}</span>
      {%- endif -%}
    </a>
    {%- if request.page_type == 'index' -%}
      </h1>
    {%- else %}
      </h2>
    {%- endif -%}
    {%- if section.settings.menu != blank and section.settings.logo_position != 'top-center' -%}
      <nav class="header__inline-menu">
        <ul class="list-menu list-menu--inline" role="list">
          {%- for link in section.settings.menu.links -%}
            <li class="hover-nav hover-enable" link-level="1">
              {%- if link.links != blank -%}
                {%- liquid
                  assign current_index = forloop.index
                  assign has_sub_menu = false
                  assign mega_menu = false
                  assign megamenu_block_index = false
                  assign show_category_image = false

                  if link.links.size > 0
                    assign has_sub_menu = true
                  endif
                   -%}
                  {%- for block in section.blocks -%}
                  {%- assign position = block.settings.position | plus: 0 -%}
                    {%- if position == current_index -%}
                      {%- assign mega_menu = true -%}
                      {%- assign megamenu_block_index = forloop.index0 -%}
                      {%- assign show_category_image = block.settings.category_image -%}
                    {%- endif -%}
                  {%- endfor -%}
                {% unless section.settings.menu_hover -%}
                  <details-disclosure open data-nav-content="{{ link.title | escape }}">
                    <details
                      class="{% if has_sub_menu == true and mega_menu == false %}dropdown-deatils{% endif %}"
                      id="Details-HeaderMenu-{{ forloop.index }}"
                      open
                    >
                {% else %}
                  <div></div>
                {% endunless %}
                <summary data-nav-content="{{ link.title | escape }}" data-megamenu="{{ mega_menu }}" class="header__menu-item list-menu__item link focus-inset 1" id="hover-enable">
                  <a href="{{ link.url }}" class="header__active-menu-item heading-font-weight">
                    {{- link.title | escape -}}
                  </a>
                </summary>
                      
                {% if has_sub_menu == true and mega_menu == true %}
                  <div class="sub-menu__content page-width-desktop sub-megamenu__container">
                    <ul
                      class="header__submenu list-menu sub-menu__level2 list-menu--disclosure caption-large motion-reduce"
                      role="list"
                      tabindex="-1"
                    >
                      {%- for childlink in link.links -%}
                        <li
                          class="sub-menu__containers{%- if show_category_image %} category-image__show{%- endif -%}"
                          link-level="2"
                        >
                          {%- if childlink.links == blank -%}
                            <a
                              href="{{ childlink.url }}"
                              class="header__menu-item list-menu__item link link--text focus-inset heading-font-weight caption-large{% if childlink.current %} list-menu__item--active{% endif %}"
                              {% if childlink.current %}
                                aria-current="page"
                              {% endif %}
                            >
                              {{ childlink.title | escape }}
                            </a>
                          {%- else -%}
                            {% assign collection_handle = childlink.url | split: '/collections/' | last %}
                            {% assign this_collection = collections[collection_handle] %}
                            <a
                              {{ collection_handle }}
                              href="{{ childlink.url }}"
                              class="header__menu-item link link--text list-menu__item focus-inset heading-font-weight caption-large"
                            >
                              {{- childlink.title | escape -}}
                            </a>
                            {% if this_collection.featured_image != blank and show_category_image %}
                              <a class="menu-category__image heading-font-weight" href="{{ childlink.url }}">
                                <img
                                  srcset="
                                    {%- if this_collection.featured_image.width >= 550 -%}{{ this_collection.featured_image | image_url: width: 550 }} 550w,{%- endif -%}
                                    {%- if this_collection.featured_image.width >= 720 -%}{{ this_collection.featured_image | image_url: width: 720 }} 720w,{%- endif -%}
                                    {%- if this_collection.featured_image.width >= 990 -%}{{ this_collection.featured_image | image_url: width: 990 }} 990w,{%- endif -%}
                                    {%- if this_collection.featured_image.width >= 1100 -%}{{ this_collection.featured_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                    {%- if this_collection.featured_image.width >= 1500 -%}{{ this_collection.featured_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                    {%- if this_collection.featured_image.width >= 2200 -%}{{ this_collection.featured_image | image_url: width: 2200 }} 2200w,{%- endif -%}
                                    {%- if this_collection.featured_image.width >= 3000 -%}{{ this_collection.featured_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                    {{ this_collection.featured_image | image_url }} {{ this_collection.featured_image.width }}w
                                  "
                                  src="{{ this_collection.featured_image | image_url: width: 1500 }}"
                                  sizes="(min-width: {{ settings.page_width }}px) {% if section.blocks.size == 1 %}calc({{ settings.page_width }}px - 100px){% else %}{{ settings.page_width | minus: 100 | times: 0.67 | round }}px{% endif %}, (min-width: 750px){% if section.blocks.size == 1 %} calc(100vw - 100px){% else %} 500px{% endif %}, calc(100vw - 30px)"
                                  alt="{{ this_collection.description | escape }}"
                                  loading="lazy"
                                  width="{{ this_collection.featured_image.width }}"
                                  height="{{ this_collection.featured_image.height }}"
                                >
                              </a>
                            {% endif %}
                            <ul class="header__submenu list-menu motion-reduce">
                              {%- for grandchildlink in childlink.links -%}
                                <li link-level="3">
                                  <a
                                    href="{{ grandchildlink.url }}"
                                    class="header__menu-item list-menu__item link link--text focus-inset body-font-weight caption-large{% if grandchildlink.current %} list-menu__item--active{% endif %}"
                                    {% if grandchildlink.current %}
                                      aria-current="page"
                                    {% endif %}
                                  >
                                    {{ grandchildlink.title | escape }}
                                  </a>
                                </li>
                              {%- endfor -%}
                            </ul>
                          {%- endif -%}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </div>
                {% else %}
                  <div class="sub-menu__content dropdown-menu">
                    <ul class="dropdown-sub__menu list-unstyled level-one__dropdown" tabindex="-1">
                      {%- for childlink in link.links -%}
                        {%- liquid
                          assign has_sub_menu = false
                          assign has_mega_menu = false
                          if childlink.links.size > 0
                            assign has_sub_menu = true
                          endif
                        -%}
                        <li
                          class="dropdown-sub__menu-item{% if childlink.active %} active{% endif %}{% if has_sub_menu %} menu-item-has-children{%- endif -%}"
                          role="none"
                          {% if has_sub_menu %}
                            data-item-title="{{ childlink.title | escape }}"
                          {% endif %}
                        >
                          <a
                            class="flex-space-center unstyled-link heading-font-weight"
                            href="{{ childlink.url }}"
                            title="{{ childlink.title | escape }}"
                            role="menuitem"
                          >
                            <span>{{- childlink.title -}}</span>
                            <span>
                              {%- if has_sub_menu -%}{% render 'icon-caret-nav' %}{%- endif -%}
                            </span>
                          </a>
                          {%- if has_sub_menu -%}
                            <ul class="dropdown-sub__menu list-unstyled level-two__dropdown" role="menu">
                              {%- for grandchildlink in childlink.links -%}
                                <li
                                  class="dropdown-sub__menu-item{% if grandchildlink.active %} active{% endif %}"
                                  role="none"
                                >
                                  <a
                                    class="unstyled-link heading-font-weight"
                                    href="{{ grandchildlink.url }}"
                                    title="{{ grandchildlink.title | escape }}"
                                    role="menuitem"
                                  >
                                    {{- grandchildlink.title -}}
                                  </a>
                                </li>
                              {%- endfor -%}
                            </ul>
                          {%- endif -%}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </div>
                {% endif %}
                {% unless section.settings.menu_hover %}
                  </details>
                  </details-disclosure>
                {% else %}
                  <div></div>
                {% endunless %}
              {%- else -%}
                <a
                  href="{{ link.url }}"
                  class="no-sub__links heading-font-weight header__menu-item header__menu-item list-menu__item link link--text focus-inset"
                  {% if link.current %}
                    aria-current="page"
                  {% endif %}
                  data-nav-content="{{ link.title | escape }}"
                >
                  <span
                    {%- if link.links %}
                      class="header__active-menu-item heading-font-weight"
                    {% endif %}
                  >
                    {{- link.title | escape -}}
                  </span>
                </a>
              {%- endif -%}
            </li>
          {%- endfor -%}
        </ul>
      </nav>
    {%- endif -%}
    <div class="header__icons  {% if section.settings.hide_account_icon %}hide-account-fix{% endif %}">
      {% render 'header-search', input_id: 'Search-In-Modal' %}

      {%- if section.settings.enable_wishlist_icon -%}
        <span class="desktop-wishlist">
          <a
            href="{%- if customer -%}{{ pages['wishlist'].url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
            class="navigation__link header__icon link link--text focus-inset"
          >
            {%- render 'icon-heart' -%}
            <span class="visually-hidden">{{ link.title | escape }} Favourite</span></a
          >
        </span>
      {%- endif -%}
      {%- if shop.customer_accounts_enabled -%}
        <span class="cart-icon-header">
          <a
            href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
            class="header__icon header__icon--account link link--text focus-inset {% if section.settings.hide_account_icon %}hide-account{% endif %}"
          >
            {% render 'icon-account' %}
            <span class="visually-hidden">
              {%- liquid
                if customer
                  echo 'customer.account_fallback' | t
                else
                  echo 'customer.log_in' | t
                endif
              -%}
            </span>
          </a>
        </span>
      {%- endif -%}
      <span class="cart-icon-header">
        <a
          href="{{ routes.cart_url }}"
          class="header__icon header__icon--cart link link--text focus-inset"
          id="cart-icon-bubble"
        >
          {%- liquid
            if cart == empty
              render 'icon-cart-empty'
            else
              render 'icon-cart'
            endif
          -%}
          <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
          {%- if cart != empty -%}{%- endif -%}
          <div class="cart-count-bubble">
            {%- if cart.item_count < 100 -%} {%- endif -%}
